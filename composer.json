{
  "name": "kleijnweb/swagger-bundle",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "John Kleijn",
      "email": "john@kleijnweb.nl",
      "homepage": "http://www.kleijnweb.nl"
    }
  ],
  "license": "LGPL-3.0",
  "description": "Makes your Swagger definition into a routing and validation spec",
  "autoload": {
    "psr-4": {
      "KleijnWeb\\SwaggerBundle\\Tests\\": "tests",
      "KleijnWeb\\SwaggerBundle\\": "src"
    }
  },
  "require": {
    "php": "^7.0.0",
    "psr/log": "1.0",
    "symfony/dependency-injection": ">=2.8.7",
    "symfony/config": ">=2.8.7",
    "symfony/event-dispatcher": ">=2.8.7",
    "symfony/http-foundation": ">=2.8.7",
    "symfony/http-kernel": ">=2.8.7",
    "symfony/filesystem": ">=2.8.7",
    "symfony/routing": ">=2.8.7",
    "symfony/yaml": ">=2.8.7",
    "symfony/finder": ">=2.8.7",
    "symfony/property-access": ">=2.8.7",
    "doctrine/collections": "^1.3",
    "doctrine/cache": "^1.6",
    "kleijnweb/php-api-descriptions": "dev-master",
    "kleijnweb/php-api-hydrator": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "symfony/framework-bundle": ">=2.8.7",
    "symfony/console": ">=2.8.7",
    "symfony/serializer": ">=2.8.7",
    "symfony/monolog-bridge": ">=2.8.7",
    "symfony/monolog-bundle": "^2.8",
    "symfony/browser-kit": ">=2.8.7",
    "mikey179/vfsStream": "^1.6",
    "phpoption/phpoption": "^1.1.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "config": {
    "bin-dir": "bin"
  }
}
